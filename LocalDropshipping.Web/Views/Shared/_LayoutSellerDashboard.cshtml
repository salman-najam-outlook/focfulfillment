    <script>

        $(document).ready(function () {
            var currentUrl = window.location.pathname;

            $(".nav-link").removeClass("active").attr("aria-selected", "false");

            if (currentUrl.includes("/Seller/SellerOrders")) {
                $("#pills-orders-tab").addClass("active").attr("aria-selected", "true");
            }
            else if (currentUrl.includes("/Seller/Withdrawal")) {
                $("#pills-withdrawals-tab").addClass("active").attr("aria-selected", "true");
            }
            else if (currentUrl.includes("/Seller/Wishlist")) {
                $("#pills-wishlist-tab").addClass("active").attr("aria-selected", "true");
            }
            else if (currentUrl.includes("/Seller/Reports")) {
                $("#pills-report-tab").addClass("active").attr("aria-selected", "true");
            }
            else if (currentUrl.includes("/Seller/Profile")) {
                $("#pills-profile-tab").addClass("active").attr("aria-selected", "true");
            }
            else {
                $("#pills-dashboard-tab").addClass("active").attr("aria-selected", "true");
            }

            $(".nav-link").click(function () {
                $(".nav-link").removeClass("active").attr("aria-selected", "false");

                $(this).addClass("active").attr("aria-selected", "true");
            });
        });

        $(".cartBtn").mouseover(function () {
            $.ajax({
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                url: '/Seller/GetCartItems',
                success: function (data) {
                    $('#exampleModal').html(data);
                    $('#exampleModal').modal({
                        backdrop: false
                    });
                },
                error: function (data) {
                    alert("Error is facing for fetching data");
                }
            });
        });
    </script>
