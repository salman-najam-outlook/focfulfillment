    <script>
        function UpdateUI(quantity, price, discountedPrice, variantId) {
            $('.data-pprice').text('Rs ' + price + '/-');
            $('.data-pdprice').text('Rs ' + discountedPrice + '/-');
              $.ajax(
                {
                    async: true,
                    type: "GET",
                    contentType: false,
                    processData: false,
                    url: "/Admin/ProductVariantImageSlider/" + variantId,
                    success: function (data) {
                        console.dir(data);
                        $('#sliderContainer').html(data);
                        $('.product-main').slick({
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            arrows: false,
                            fade: true,
                            asNavFor: '.left-slider-image'
                        });

                        $('.left-slider-image').slick({
                            slidesToShow: 4,
                            slidesToScroll: 1,
                            asNavFor: '.product-main',
                            dots: false,
                            focusOnSelect: true,
                            // vertical: false,
                            responsive: [{
                                breakpoint: 1400,
                                settings: {
                                    vertical: false,
                                }
                            },
                            {
                                breakpoint: 992,
                                settings: {
                                    vertical: false,
                                }
                            },
                            {
                                breakpoint: 768,
                                settings: {
                                    vertical: false,
                                }
                            }, {
                                breakpoint: 430,
                                settings: {
                                    slidesToShow: 3,
                                    vertical: false,
                                }
                            },
                            ]
                        });
                    },
                    error: function () {    
                        alert("This Specific Variant Does Not Exist");
                    }
                }
            );
        }
    </script>

